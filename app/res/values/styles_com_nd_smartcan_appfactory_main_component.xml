<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="maincomponent_AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.




        -->
    </style>

    <!-- Application theme. -->
    <style name="maincomponent_AppTheme" parent="maincomponent_AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="maincomponent_main_foot_tab_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/skin_maincomponent_10sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/maincom_foot_text_color</item>
    </style>

    <style name="maincomponent_jay_main_foot_count">
        <item name="android:textSize">@dimen/skin_maincomponent_10sp</item>
        <item name="android:textColor">@color/skin_maincomponent_white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    <!-- 设置页面 与个人信息左侧  字体 -->
    <style name="maincomponent_setting_txt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 设置页面   图片 -->
    <style name="maincomponent_setting_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>
    <!-- 设置页面布局 -->
    <style name="maincomponent_setting_layout_item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/skin_maincomponent_setting_item_bg</item>
    </style>
    <!-- 横向分割符 -->
    <style name="maincomponent_task_separateline_horizontal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:background">@drawable/skin_maincomponent_setting_separateline_horizontal</item>
    </style>
    <!-- 半透明主题 -->
    <style name="Transparent">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowBackground">@color/skin_maincomponent_theme_transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="UpdateDialogStyle">
        <item name="android:windowBackground">@color/skin_maincomponent_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="AppfactoryDevToolStyle">
        <item name="android:layout_marginTop">@dimen/skin_maincomponent_dp10</item>
        <item name="android:layout_marginEnd">@dimen/skin_maincomponent_dp10</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>

    <style name="MainComDevToolStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/skin_maincomponent_dp40</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/skin_maincomponent_20sp</item>
        <item name="android:layout_marginStart">@dimen/skin_maincomponent_dp10</item>

    </style>

    <style name="devtool_UIMonitor_config">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="devtool_UIMonitor">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="resign_check_DialogStyle">
        <item name="android:windowBackground">@color/resign_check_dialog_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
	<!--整个webview界面的主题-->
	<style name="ApfWebView">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
     <!--webview界面页面查找中三个按钮的主题-->
    <style name="webcomponent.Feature.FindInPage.Buttons" parent="">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="tint">#ff000000</item>
    </style>

    <style name="maincomponent_maintenance_advance_notice_dialog_style">
        <item name="android:windowBackground">@color/skin_maincomponent_half_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>　　　　　　　
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
	
	
	
    <style name="Theme.AppCompat.Translucent.Screen" parent="Theme.AppCompat">
        <item name="android:windowBackground">@color/webcomponent_screen_pre_bg_color</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/ScreenAnimation</item>
    </style>

    <style name="ScreenAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>

</resources>